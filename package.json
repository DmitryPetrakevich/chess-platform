{
  "name": "chess-platform",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" --names \"BACK,FRONT\" --prefix-colors \"bgRed.white.bold,bgBlue.white.bold\"",
    "dev:backend": "cd chess-platform-backend && npm run dev",
    "dev:frontend": "cd chess-platform-frontend && npm run dev",
    "start": "npm run dev",
    "build": "cd chess-platform-frontend && npm run build",
    "preview": "cd chess-platform-frontend && npm run preview"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "chess.js": "^1.4.0",
    "jsonwebtoken": "^9.0.3",
    "rss-parser": "^3.13.0"
  }
}
